{
  "name": "nextjs-starter-template",
  "version": "0.1.0",
  "private": false,
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "scripts": {
    "requirements-check": "ts-node --compilerOptions='{\"module\":\"commonjs\"}' src/application/support/enforce-node-version.ts",
    "setup-env": "npm run requirements-check",
    "preinstall": "npm run setup-env",
    "predev": "npm run setup-env",
    "prestart": "npm run setup-env",
    "prebuild": "npm run setup-env",
    "pretest": "npm run setup-env",
    "prelint": "npm run setup-env",
    "preeslint-fix": "npm run setup-env",
    "pretsc": "npm run setup-env",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && stylelint \"./src/**/*.{scss,css}\" && prettier -c .",
    "eslint-fix": "npx eslint . --fix",
    "test": "jest --watchAll",
    "test:ci": "jest --ci",
    "tsc": "tsc --project ./tsconfig.json",
    "tsc:watch": "tsc --project ./tsconfig.json -w"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "next": "^13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "~1.55.0",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-syntax-jsx": "^7.21.4",
    "@babel/runtime": "^7.20.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.4.1",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.28",
    "@types/react-test-renderer": "^18.0.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "babel-loader": "8.2.5",
    "eslint": "8.24.0",
    "eslint-config-next": "13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.2.2",
    "jest-environment-node-single-context": "^29.0.0",
    "jest-expect-message": "^1.1.3",
    "jest-runner-eslint": "^1.1.0",
    "jest-runner-prettier": "^1.0.0",
    "jest-runner-stylelint": "^2.3.7",
    "postcss": "^8.4.20",
    "prettier": "2.7.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-test-renderer": "^18.2.0",
    "sass-true": "^7.0.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
